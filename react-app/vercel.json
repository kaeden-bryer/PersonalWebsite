{
  "rewrites": [
    {
      "source": "/blog",
      "destination": "https://kaedens-blog.vercel.app/"
    },
    {
      "source": "/blog/",
      "destination": "https://kaedens-blog.vercel.app/"
    },
    {
      "source": "/blog/(.*)",
      "destination": "https://kaedens-blog.vercel.app/$1"
    },
    {
      "source": "/((?!blog).*)",
      "destination": "/index.html"
    }
  ]
}